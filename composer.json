{
    "name": "sirix/cycle-orm-factory",
    "description": "Cycle ORM Factories for Mezzio",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name":     "Sirix",
            "homepage": "https://github.com/sirix777"
        }
    ],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "laminas": {
            "config-provider": "Sirix\\Cycle\\ConfigProvider"
        }
    },
    "require": {
        "php": "~8.1.0 || ~8.2.0",
        "composer/package-versions-deprecated": "^1.10.99",
        "cycle/annotated": "^3.3",
        "cycle/database": "^2.5",
        "cycle/entity-behavior": "^1.2",
        "cycle/entity-behavior-uuid": "^1.1",
        "cycle/migrations": "^4.1",
        "cycle/orm": "^2.3",
        "cycle/schema-migrations-generator": "^2.1",
        "symfony/cache": "^6.3",
        "symfony/console": "^6.3"
    },
    "require-dev": {
        "laminas/laminas-coding-standard": "^2.5",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Sirix\\Cycle\\": "src/"
        }
    },
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf"
    }
}
